{
  "name": "finc-api",
  "version": "0.0.1",
  "description": "API NestJS para notificações com Firebase e RabbitMQ",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "node --max-old-space-size=1024 dist/main.js",
    "start:dev": "nest start --watch",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^4.0.0",
    "@nestjs/common": "^10.2.5",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.2.5",
    "@nestjs/platform-express": "^10.2.5",
    "firebase-admin": "^12.11.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.5",
    "@nestjs/schematics": "^10.2.5",
    "@nestjs/testing": "^10.2.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.6.2",
    "@types/node": "^20.5.0",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.2.0",
    "jest": "^29.6.2",
    "prettier": "^3.9.1",
    "rimraf": "^5.1.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": "20.x"
  }
}
